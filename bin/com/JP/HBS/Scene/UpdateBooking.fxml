<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="675.0"
	prefWidth="950.0" style="-fx-background-color: #ffffff;"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.JP.HBS.Controller.UpdateBookingController">
	<children>
		<JFXButton defaultButton="true"
			onAction="#onBackButtonClick" prefHeight="50.0" prefWidth="50.0"
			style="-fx-background-color: transparent; -fx-background-radius: 30;"
			text="&lt;" textFill="#48a5ea" AnchorPane.topAnchor="10.0">
			<font>
				<Font name="Segoe UI Bold" size="22.0" />
			</font>
		</JFXButton>
		<Label text="Room Details" textFill="#48a5ea"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="50.0">
			<font>
				<Font name="Segoe UI Bold" size="24.0" />
			</font>
		</Label>
		<Label fx:id="bookingIDLabel" textFill="#cacaca"
			AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0">
			<font>
				<Font name="Segoe UI Bold" size="24.0" />
			</font>
		</Label>
		<JFXComboBox fx:id="roomCombobox" focusColor="#8ac3f0"
			labelFloat="true" prefHeight="35.0" prefWidth="350.0"
			promptText="Room Number" unFocusColor="#eeeeee"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="100.0" />
		<DatePicker fx:id="dateDatePicker" editable="false"
			prefHeight="35.0" prefWidth="350.0" promptText="Date"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="170.0" />
		<JFXTextField fx:id="durationTextfield"
			focusColor="#8ac3f0" labelFloat="true" prefHeight="35.0"
			prefWidth="350.0" promptText="Days of Stay" unFocusColor="#eeeeee"
			AnchorPane.leftAnchor="500.0" AnchorPane.topAnchor="170.0">
			<font>
				<Font name="Segoe UI" size="14.0" />
			</font>
		</JFXTextField>
		<Label text="Customer Details" textFill="#48a5ea"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="250.0">
			<font>
				<Font name="Segoe UI Bold" size="24.0" />
			</font>
		</Label>
		<JFXTextField fx:id="nameTextfield" focusColor="#8ac3f0"
			labelFloat="true" prefHeight="35.0" prefWidth="350.0"
			promptText="Full Name" unFocusColor="#eeeeee"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="300.0">
			<font>
				<Font name="Segoe UI" size="14.0" />
			</font>
		</JFXTextField>
		<JFXTextField fx:id="icTextfield" focusColor="#8ac3f0"
			labelFloat="true" prefHeight="35.0" prefWidth="350.0"
			promptText="IC/Passport" unFocusColor="#eeeeee"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="370.0">
			<font>
				<Font name="Segoe UI" size="14.0" />
			</font>
		</JFXTextField>
		<JFXTextField fx:id="contact1Textfield"
			focusColor="#8ac3f0" labelFloat="true" prefHeight="35.0"
			prefWidth="350.0" promptText="Contact Number 1"
			unFocusColor="#eeeeee" AnchorPane.leftAnchor="500.0"
			AnchorPane.topAnchor="370.0">
			<font>
				<Font name="Segoe UI" size="14.0" />
			</font>
		</JFXTextField>
		<JFXTextField fx:id="emailTextfield" focusColor="#8ac3f0"
			labelFloat="true" prefHeight="35.0" prefWidth="350.0"
			promptText="Email Address" unFocusColor="#eeeeee"
			AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="440.0">
			<font>
				<Font name="Segoe UI" size="14.0" />
			</font>
		</JFXTextField>
		<JFXTextField fx:id="contact2Textfield"
			focusColor="#8ac3f0" labelFloat="true" prefHeight="35.0"
			prefWidth="350.0" promptText="Contact Number 2"
			unFocusColor="#eeeeee" AnchorPane.leftAnchor="500.0"
			AnchorPane.topAnchor="440.0">
			<font>
				<Font name="Segoe UI" size="14.0" />
			</font>
		</JFXTextField>
		<JFXButton defaultButton="true" onAction="#onSubmit"
			prefHeight="50.0" prefWidth="200.0"
			style="-fx-background-color: #48A5EA; -fx-background-radius: 30;"
			text="UPDATE" textFill="WHITE" AnchorPane.bottomAnchor="50.0"
			AnchorPane.rightAnchor="50.0">
			<font>
				<Font name="Segoe UI Bold" size="16.0" />
			</font>
		</JFXButton>
		<JFXButton alignment="BASELINE_LEFT"
			onAction="#onButtonReset" prefHeight="50.0"
			style="-fx-background-color: transparent;" text="Reset All"
			textFill="#cacaca" AnchorPane.bottomAnchor="50.0"
			AnchorPane.leftAnchor="50.0">
			<font>
				<Font name="Segoe UI Bold" size="16.0" />
			</font>
		</JFXButton>
	</children>
</AnchorPane>
